<?xml version="1.0" encoding="UTF-8"?>
<mixin>
  <name>mySet</name>
  <module>myModule</module>
  <display-name>My Mixin</display-name>
  <form-item-set name="mySet">
    <label>My set</label>
    <immutable>false</immutable>
    <customText>Custom text</customText>
    <helpText>Custom text</helpText>
    <occurrences minimum="1" maximum="3"/>
    <items>
      <input type="TextLine" built-in="true" name="myInput">
        <label>My input</label>
        <immutable>false</immutable>
        <indexed>false</indexed>
        <customText/>
        <helpText/>
        <occurrences minimum="0" maximum="1"/>
      </input>
    </items>
  </form-item-set>
</mixin>