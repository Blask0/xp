<?xml version="1.0" encoding="UTF-8"?>
<type>
  <name>all_schemas</name>
  <display-name>All the Base Types</display-name>
  <content-display-name-script>$('firstName') + ' ' + $('lastName')</content-display-name-script>
  <super-type>content</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <is-built-in>false</is-built-in>
  <allow-child-content>true</allow-child-content>
  <form>
    <form-item-set name="mySet">
      <label/>
      <immutable>false</immutable>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
      <items>
        <layout name="myFieldSet" type="FieldSet">
          <label>My field set</label>
          <items>
            <input type="TextLine" name="myTextLine">
              <label/>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <custom-text/>
              <help-text/>
              <occurrences minimum="0" maximum="1"/>
            </input>
          </items>
        </layout>
        <mixin-reference name="myCommonInput">
          <name>myCommonInput</name>
          <reference>my_shared_input</reference>
        </mixin-reference>
      </items>
    </form-item-set>
  </form>
</type>

