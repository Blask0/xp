<?xml version="1.0" encoding="UTF-8"?>
<type>
  <name>AllBaseTypes</name>
  <module>myModule</module>
  <qualifiedName>myModule:AllBaseTypes</qualifiedName>
  <displayName>All the Base Types</displayName>
  <superType>System:Content</superType>
  <isAbstract>false</isAbstract>
  <isFinal>true</isFinal>
  <items>
    <form-item-set name="mySet">
      <label/>
      <immutable>false</immutable>
      <customText/>
      <helpText/>
      <occurrences minimum="0" maximum="1"/>
      <items>
        <layout name="myFieldSet" type="FieldSet">
          <label>My field set</label>
          <items>
            <input type="TextLine" built-in="true" name="myTextLine">
              <label/>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <customText/>
              <helpText/>
              <occurrences minimum="0" maximum="1"/>
            </input>
          </items>
        </layout>
        <sub-type-reference name="myCommonInput">
          <name>myCommonInput</name>
          <reference>System:mySharedInput</reference>
          <type>InputSubType</type>
        </sub-type-reference>
      </items>
    </form-item-set>
  </items>
</type>

