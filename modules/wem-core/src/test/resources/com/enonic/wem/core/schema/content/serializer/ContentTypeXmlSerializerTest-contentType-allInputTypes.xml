        <relationship-type>system:like</relationship-type>
      </config>
    </input>
    <input type="ImageSelector" name="myImage">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
      <config>
        <relationship-type>system:default</relationship-type>
      </config>
    </input>
    <input type="custom:DummyCustomInputType" name="myCustomInput">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
