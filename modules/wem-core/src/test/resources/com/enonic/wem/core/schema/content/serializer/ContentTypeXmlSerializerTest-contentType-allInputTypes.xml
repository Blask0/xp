<?xml version="1.0" encoding="UTF-8"?>
<type>
  <name>all_input_types</name>
  <module>mymodule</module>
  <display-name>All the Input Types</display-name>
  <content-display-name-script/>
  <super-type>system:content</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <allow-children>true</allow-children>
  <form>
    <input type="Color" name="myColor">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="Date" name="myDate">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="DecimalNumber" name="myDecimalNumber">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="GeoLocation" name="myGeoLocation">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="HtmlArea" name="myHtmlArea">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
