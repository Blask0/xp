    <input type="TextArea" name="myTextArea">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input type="WholeNumber" name="myWholeNumber">
      <label/>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
