<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<page-component xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema/model.xsd">
  <display-name>Landing page</display-name>
  <config>
    <input name="pause" type="DecimalNumber">
      <label>Pause parameter</label>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <form-item-set name="myFormItemSet">
      <label>My form item set</label>
      <items>
        <input name="myTextLine" type="TextLine">
          <label>My text line</label>
          <immutable>false</immutable>
          <indexed>false</indexed>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input name="myCustomInput" type="TextLine">
          <label>My custom input</label>
          <immutable>false</immutable>
          <indexed>false</indexed>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <field-set name="myFieldSet">
          <label>My field set</label>
          <items>
            <input name="fieldSetItem" type="TextLine">
              <label>Field set Item</label>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <occurrences minimum="0" maximum="1"/>
            </input>
          </items>
        </field-set>
      </items>
      <immutable>false</immutable>
      <occurrences minimum="0" maximum="1"/>
    </form-item-set>
  </config>
  <regions>
    <region name="header"/>
    <region name="main"/>
    <region name="footer"/>
  </regions>
</page-component>
