<?xml version="1.0" encoding="UTF-8"?>
<page-template>
  <display-name>Main page template</display-name>
  <descriptor>mainmodule:landing-page</descriptor>
  <config>
    <pause type="Long">10000</pause>
    <thing type="DataSet">
      <first type="String">one</first>
      <second type="String">two</second>
    </thing>
  </config>
  <can-render>
    <content-type>mymodule:com.enonic.sometype</content-type>
    <content-type>mymodule:some.other.type</content-type>
  </can-render>
  <regions>
    <region name="my-region">
      <part-component descriptor="demo:my-part-template" name="PartInHeader">
        <config>
          <width type="Long">500</width>
          <caption type="String">So sweet!</caption>
        </config>
      </part-component>
    </region>
  </regions>
</page-template>
