<?xml version="1.0" encoding="UTF-8"?>
<page-template>
  <name>main-page</name>
  <display-name>Main page template</display-name>
  <descriptor>mainmodule-1.0.0:landing-page</descriptor>
  <config>
    <property name="pause" type="Long">10000</property>
    <data-set name="thing">
      <property name="first" type="String">one</property>
      <property name="second" type="String">two</property>
    </data-set>
  </config>
  <can-render>
    <content-type>com.enonic.sometype</content-type>
    <content-type>some.other.type</content-type>
  </can-render>
  <regions>
    <region>
      <property name="name" type="String">my-region</property>
      <data-set name="PartComponent">
        <property name="template" type="String">BluemanIntra-1.0.0|demo-1.0.0|my-part-template</property>
        <property name="name" type="String">PartInHeader</property>
        <property name="config" type="Data">
          <data>
            <property name="width" type="Long">500</property>
            <property name="caption" type="String">So sweet!</property>
          </data>
        </property>
      </data-set>
    </region>
  </regions>
</page-template>
