<?xml version="1.0" encoding="UTF-8"?>
<form>
  <input type="Double" name="pause">
  <label>Pause parameter</label>
    <immutable>true</immutable>
    <indexed>true</indexed>
    <custom-text>customText</custom-text>
    <help-text>helpText</help-text>
    <validation-regexp>!@$%</validation-regexp>
    <occurrences minimum="2" maximum="3"/>
  </input>
  <form-item-set name="myFormItemSet">
    <label>My form item set</label>
    <immutable>false</immutable>
    <occurrences minimum="0" maximum="1"/>
    <items>
      <input type="TextLine" name="myTextLine">
        <label>My text line</label>
        <immutable>false</immutable>
        <indexed>false</indexed>
        <occurrences minimum="1" maximum="1"/>
      </input>
      <input type="TextLine" name="myCustomInput">
        <label>My custom input</label>
        <immutable>false</immutable>
        <indexed>false</indexed>
        <occurrences minimum="0" maximum="1"/>
      </input>
      <field-set name="myFieldSet">
        <label>My field set</label>
        <items>
          <input type="TextLine" name="fieldSetItem">
            <label>Field set Item</label>
            <immutable>false</immutable>
            <indexed>false</indexed>
            <occurrences minimum="0" maximum="1"/>
          </input>
        </items>
      </field-set>
    </items>
  </form-item-set>
  <mixin-reference name="mixin">
    <reference>reference</reference>
  </mixin-reference>
</form>
