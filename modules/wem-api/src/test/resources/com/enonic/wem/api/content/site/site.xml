<?xml version="1.0" encoding="UTF-8"?>
<site-template>
  <display-name>Enonic Intranet</display-name>
  <description>A social intranet for the Enterprise</description>
  <url>http://www.enonic.com/intranet</url>
  <vendor>
    <name>Enonic</name>
    <url>https://www.enonic.com</url>
  </vendor>
  <modules>
    <module>com.enonic.intranet</module>
    <module>com.company.sampleModule</module>
    <module>com.company.theme.someTheme</module>
    <module>com.enonic.resolvers</module>
  </modules>
  <content-filter>
    <deny>*</deny>
    <allow>mymodule:com.enonic.tweet</allow>
    <allow>mymodule:system.folder</allow>
  </content-filter>
</site-template>