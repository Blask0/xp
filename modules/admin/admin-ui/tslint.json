{
  "rules": {
    "forin": true,
    "label-position": true,
    "no-any": false,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "debug",
      "info",
      "log",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-invalid-this": true,
    "no-string-literal": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-new": true,
    "no-use-before-declare": true,
    "radix": true,
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "use-isnan": true,
    "arrow-parens": false,
    "class-name": true,
    "comment-format": true,
    "interface-name": true,
    "jsdoc-format": true,
    "max-file-line-count": true,
    "max-line-length": [
      true,
      140
    ],
    "member-access": false,
    "member-ordering": false,
    "new-parens": true,
    "no-construct": true,
    "no-default-export": true,
    "no-inferrable-types": false,
    "no-null-keyword": false,
    "no-require-imports": true,
    "no-shadowed-variable": true,
    "no-var-keyword": true,
    "no-var-requires": true,
    "object-literal-sort-keys": false,
    "one-variable-per-declaration": true,
    "only-arrow-functions": false,
    "ordered-imports": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ],
    "variable-name": true,
    "align": [
      true,
      "parameters",
      "arguments",
      "statements"
    ],
    "curly": true,
    "eofline": true,
    "indent": [
      true,
      "spaces"
    ],
    "linebreak-style": false,
    "no-consecutive-blank-lines": true,
    "no-trailing-whitespace": true,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
    "typedef-whitespace": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "ban": false,
    "no-angle-bracket-type-assertion": false,
    "no-internal-module": false,
    "no-mergeable-namespace": false,
    "no-namespace": false,
    "no-reference": false
  }
}
